<?xml version="1.0" encoding="UTF-8"?>
<!-- REAL FIELD DATA - 60% Scaled for Optimal Performance Study -->
<!-- Based on actual 5:30-6:30 PM measurements, scaled to 60% -->
<!-- Peak demand: ~2,328 veh/hr (near capacity) -->
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" minGap="2.5" maxSpeed="50" guiShape="passenger"/>
    
    <!-- Route Definitions -->
    <route id="route_r1_straight" edges="r1_in r1_out"/>
    <route id="route_r2_left" edges="r2_in r2_out_left"/>
    <route id="route_r2_straight" edges="r2_in r2_out_straight"/>
    <route id="route_r3_straight" edges="r3_in r3_out"/>
    
    <!--
    
    SCALED REAL TRAFFIC - 60% of Evening Rush Hour Measurements
    Original Collection Time: 5:30 PM - 6:30 PM
    Scaling Factor: 0.6 (represents "busy but manageable" traffic)
    
    
    Original vs Scaled Traffic Volumes:
    
    Period         | Original | Scaled (60%) | Hourly Equivalent
    |||
    5:30 - 5:42 PM |   776    |     466      | 2,328 veh/hr
    5:42 - 5:54 PM |   719    |     431      | 2,157 veh/hr
    5:54 - 6:06 PM |   755    |     453      | 2,265 veh/hr
    6:06 - 6:18 PM |   589    |     353      | 1,767 veh/hr
    6:18 - 6:30 PM |   529    |     317      | 1,587 veh/hr
    |||
    TOTAL          |  3,368   |    2,021     | 2,021 veh/hr avg
    
    -->
    
    <!--  -->
    <!-- PERIOD 1: 5:30-5:42 PM (0-720s)            -->
    <!-- 60% of 776 vehicles = 466 veh/12min        -->
    <!-- Equivalent: 2,328 veh/hr                   -->
    <!--  -->
    
    <!-- R1: 343 × 0.6 = 206 veh/12min = 1,029 veh/hr -->
    <flow id="flow_r1_period1" type="car" route="route_r1_straight" 
          begin="0" end="720" vehsPerHour="1029" departLane="best" departSpeed="max"/>
    
    <!-- R2 Left: 45 × 0.6 = 27 veh/12min = 135 veh/hr -->
    <flow id="flow_r2_left_period1" type="car" route="route_r2_left" 
          begin="0" end="720" vehsPerHour="135" departLane="0" departSpeed="max"/>
    
    <!-- R2 Straight: 268 × 0.6 = 161 veh/12min = 804 veh/hr -->
    <flow id="flow_r2_straight_period1" type="car" route="route_r2_straight" 
          begin="0" end="720" vehsPerHour="804" departLane="1" departSpeed="max"/>
    
    <!-- R3: 120 × 0.6 = 72 veh/12min = 360 veh/hr -->
    <flow id="flow_r3_period1" type="car" route="route_r3_straight" 
          begin="0" end="720" vehsPerHour="360" departLane="best" departSpeed="max"/>
    
    <!--  -->
    <!-- PERIOD 2: 5:42-5:54 PM (720-1440s)         -->
    <!-- 60% of 719 vehicles = 431 veh/12min        -->
    <!-- Equivalent: 2,157 veh/hr                   -->
    <!--  -->
    
    <!-- R1: 242 × 0.6 = 145 veh/12min = 726 veh/hr -->
