          begin="720" end="1440" vehsPerHour="726" departLane="best" departSpeed="max"/>
    
    <!-- R2 Left: 88 × 0.6 = 53 veh/12min = 264 veh/hr -->
    <flow id="flow_r2_left_period2" type="car" route="route_r2_left" 
          begin="720" end="1440" vehsPerHour="264" departLane="0" departSpeed="max"/>
    
    <!-- R2 Straight: 294 × 0.6 = 176 veh/12min = 882 veh/hr -->
    <flow id="flow_r2_straight_period2" type="car" route="route_r2_straight" 
          begin="720" end="1440" vehsPerHour="882" departLane="1" departSpeed="max"/>
    
    <!-- R3: 95 × 0.6 = 57 veh/12min = 285 veh/hr -->
    <flow id="flow_r3_period2" type="car" route="route_r3_straight" 
          begin="720" end="1440" vehsPerHour="285" departLane="best" departSpeed="max"/>
    
